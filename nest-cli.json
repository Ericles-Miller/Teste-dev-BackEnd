{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "tsconfig.json"
  },
  "monorepo": true,
  "root": "apps",
  "projects": {
    "challenge-flow": {
    "type": "application",
    "root": "apps/challenge-flow",
    "sourceRoot": "apps/challenge-flow/src",
    "entryFile": "src/main",
    "compilerOptions": {
      "tsConfigPath": "apps/challenge-flow/tsconfig.app.json"
    }
    },
    "rabbit-mq": {
      "type": "application",
      "root": "apps/rabbit-mq",
      "entryFile": "main",
      "sourceRoot": "apps/rabbit-mq/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rabbit-mq/tsconfig.app.json"
      }
    },
    "redis": {
      "type": "application",
      "root": "apps/redis",
      "entryFile": "main",
      "sourceRoot": "apps/redis/src",
      "compilerOptions": {
        "tsConfigPath": "apps/redis/tsconfig.app.json"
      }
    }
  }
}